{
  "name": "estructura-basica-express",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node --harmony --trace-warnings -r dotenv/config src/index.js",
    "start:dev": "cross-env NODE_ENV=local node --watch --trace-warnings -r dotenv/config src/index.js",
    "test": "NODE_ENV=development nyc --exclude=test --reporter=text mocha -r dotenv/config --recursive test/ --reporter spec ",
    "test:local": "nyc --exclude=test --reporter=lcov --reporter=text  mocha -r dotenv/config --recursive test/**/*test.js --reporter spec --exit",
    "test:coverage": "nyc report --statements 80 --functions 80 --lines 80 --branches 80 --check-coverage --reporter=text-summary --per-file;exit 0",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "lint-and-format": "npm run lint:fix && npm run format"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.9",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "morgan": "^1.10.0",
    "msw": "^2.6.8",
    "nodemon": "^3.1.7",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.9",
    "@babel/eslint-plugin": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.16.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.13.0",
    "mocha": "^11.0.1",
    "nyc": "^17.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "3.4.2",
    "sequelize-cli": "^6.6.2"
  }
}
